{
  "hosting": {
    "public": "public",
    "ignore": ["**/.*", "**/node_modules/**"],
    "headers": [
      { "source": "**/*.js", "headers": [{ "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }] }
    ],
    "rewrites": [{ "source": "**", "destination": "/index.html" }]
  }
}
